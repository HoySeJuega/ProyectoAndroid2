<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.camilo.proyectoandroid.Cancha11"
    android:background="@drawable/cancha">

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton20"
        android:background="@drawable/boton"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="Arquero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton21"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton20"
        android:layout_toStartOf="@+id/imageButton22"
        android:clickable="true"
        android:onClick="Defensor" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton22"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton21"
        android:layout_alignParentEnd="true"
        android:clickable="true"
        android:nestedScrollingEnabled="false"
        android:onClick="Defensor" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton23"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton20"
        android:layout_toStartOf="@+id/imageButton20"
        android:clickable="true"
        android:nestedScrollingEnabled="false"
        android:onClick="Defensor" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton24"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton23"
        android:layout_alignParentStart="true"
        android:clickable="true"
        android:onClick="Defensor" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton25"
        android:background="@drawable/boton"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/imageButton24"
        android:clickable="true"
        android:onClick="Delantero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton26"
        android:background="@drawable/boton"
        android:layout_alignTop="@+id/imageButton25"
        android:layout_toStartOf="@+id/imageButton22"
        android:clickable="true"
        android:onClick="Delantero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton27"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton26"
        android:layout_toEndOf="@+id/imageButton26"
        android:clickable="true"
        android:onClick="Delantero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton28"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton25"
        android:layout_toStartOf="@+id/imageButton25"
        android:clickable="true"
        android:onClick="Delantero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton29"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton28"
        android:layout_toStartOf="@+id/imageButton20"
        android:clickable="true"
        android:onClick="Delantero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton30"
        android:background="@drawable/boton"
        android:layout_alignTop="@+id/imageButton29"
        android:layout_alignStart="@+id/imageButton26"
        android:clickable="true"
        android:onClick="Delantero" />

</RelativeLayout>
