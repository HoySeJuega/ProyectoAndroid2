<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.camilo.proyectoandroid.Cancha9"
    android:background="@drawable/cancha">

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton5"
        android:background="@drawable/boton"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="Arquero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton12"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton5"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="48dp"
        android:clickable="true"
        android:onClick="Defensor" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton13"
        android:background="@drawable/boton"
        android:layout_alignTop="@+id/imageButton12"
        android:layout_alignParentEnd="true"
        android:clickable="true"
        android:onClick="Defensor" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton14"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton5"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="Defensor" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton15"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton12"
        android:layout_alignStart="@+id/imageButton14"
        android:clickable="true"
        android:onClick="Delantero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton16"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton15"
        android:layout_alignStart="@+id/imageButton13"
        android:clickable="true"
        android:onClick="Delantero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton17"
        android:background="@drawable/boton"
        android:layout_alignTop="@+id/imageButton16"
        android:layout_alignParentStart="true"
        android:clickable="true"
        android:onClick="Delantero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton18"
        android:background="@drawable/boton"
        android:layout_above="@+id/imageButton17"
        android:layout_toStartOf="@+id/imageButton15"
        android:clickable="true"
        android:onClick="Delantero" />

    <ImageButton
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:id="@+id/imageButton19"
        android:background="@drawable/boton"
        android:layout_alignTop="@+id/imageButton18"
        android:layout_toStartOf="@+id/imageButton16"
        android:clickable="true"
        android:onClick="Delantero" />

</RelativeLayout>
